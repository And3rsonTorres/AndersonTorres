generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Contact {
  id      String @id @default(auto()) @map("_id") @db.ObjectId
  Email   String
  Message String
  Name    String
}

model Project {
  id            String @id @default(auto()) @map("_id") @db.ObjectId
  AlternateName String
  description   String
  githubLink    String
  headter       String
  imagePath     String
  insideLink    String
  version       String
}
